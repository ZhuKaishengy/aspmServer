<?xml version="1.0" encoding="UTF-8" ?>

    <!DOCTYPE generatorConfiguration 
      PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
      "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <properties resource="generator/conf.properties"></properties>
    
   <classPathEntry
        location="D:\environment\workspaces\mavenRepository\com\microsoft\sqlserver\sqljdbc4\4.0\sqljdbc4-4.0.jar" />
        <!--location="D:\environment\workspaces\mavenRepository\com\oracle\ojdbc6\12.1.0.1-atlassian-hosted\ojdbc6-12.1.0.1-atlassian-hosted.jar" />-->

    <context id="mysqlTables" targetRuntime="MyBatis3">
    
        <commentGenerator>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        
        <jdbcConnection driverClass="${jdbc.driver}" connectionURL="${jdbc.url}" userId="${jdbc.username}" password="${jdbc.password}"></jdbcConnection>  
        
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

         <!-- 生成model模型，对应的包，存放位置可以指定具体的路径,如/ProjectName/src，也可以使用MAVEN来自动生成 -->
        <javaModelGenerator targetPackage="com.aisino.db2.domain"
            targetProject="${projectName}\src\main\java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        
         <!--对应的xml mapper文件  -->
        <sqlMapGenerator targetPackage="mapper" targetProject="${projectName}\src\main\resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
        
        <!-- 对应的dao接口 -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.aisino.db2.dao" targetProject="${projectName}\src\main\java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

         <!-- 这里是数据库的表名table_user 以及 POJO类的名字User -->

       	<table tableName="ASP_PRODUCT_INFO_TAB" domainObjectName="Product"></table>
       	<table tableName="MSP_SEC_USER_TAB" domainObjectName="User"></table>
       	<table tableName="ASP_AUTH_INFO_TAB" domainObjectName="Auth"></table>
        <!-- <table tableName="ASN_ROLE" domainObjectName="Role"></table>
        <table tableName="ASN_PERMISSION" domainObjectName="Permission"></table>
        <table tableName="ASN_USER_ROLE" domainObjectName="UserRole"></table>
        <table tableName="ASN_ROLE_PERMISSION" domainObjectName="RolePermission"></table>
        <table tableName="ASN_FILTERCHAINDEFENITION" domainObjectName="FilterChainDefinition"></table> -->
    </context>

</generatorConfiguration>